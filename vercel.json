{
  "version": 2,
  "builds": [
    {
      "src": "langchain-agents/app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "langchain-agents/app/main.py"
    },
    {
      "src": "/gemini/(.*)",
      "dest": "langchain-agents/app/main.py"
    },
    {
      "src": "/health",
      "dest": "langchain-agents/app/main.py"
    },
    {
      "src": "/new",
      "dest": "langchain-agents/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "langchain-agents/app/main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "AIzaSyCaonefFVlB5g3xpg9vipm0FTZJSDtcQEM",
    "GOOGLE_API_KEY": "AIzaSyCaonefFVlB5g3xpg9vipm0FTZJSDtcQEM"
  }
}
